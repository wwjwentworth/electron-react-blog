  "scripts": {
    "start": "node scripts/start.js",
    "build:dev": "cross-env DEPLOY_ENV=dev node scripts/build.js",
    "build:rc": "cross-env DEPLOY_ENV=rc node scripts/build.js",
    "build:gray": "cross-env DEPLOY_ENV=gray node scripts/build.js",
    "build:prod": "cross-env DEPLOY_ENV=prod node scripts/build.js",
    "test": "node scripts/test.js",
    "electron-start": "electron ."
  },
  "betterScripts": {
    "build:dev": {
      "command": "react-app-rewired build",
      "env": {
        "NODE_ENV": "production",
        "DEPLOY_ENV": "dev"
      }
    },
    "build:dev1": {
      "command": "react-app-rewired build",
      "env": {
        "NODE_ENV": "production",
        "DEPLOY_ENV": "dev1"
      }
    },
    "build:rc": {
      "command": "react-app-rewired build",
      "env": {
        "NODE_ENV": "production",
        "DEPLOY_ENV": "rc"
      }
    },
    "build:gray": {
      "command": "react-app-rewired build",
      "env": {
        "NODE_ENV": "production",
        "DEPLOY_ENV": "gray"
      }
    },
    "build:prod": {
      "command": "react-app-rewired build",
      "env": {
        "NODE_ENV": "production",
        "DEPLOY_ENV": "prod"
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "jsx-control-statements"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
